{
  "manifest_version": 2,
  "name": "ACE editor for sources.d.n",
  "version": "@VERSION@",
  "description": "In-browser editing of Debian sources, yay!",
  "author": "Raphael Geissert",
  "content_scripts": [
    {
	"matches": ["*://sources.debian.net/src/*"],
	"js": [
	    "loader.js"
	    ]
    }
  ],
  "update_url": "@BASE_URL@/updates.xml"
}